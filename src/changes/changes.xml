<document xmlns="http://maven.apache.org/changes/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
	<properties>
		<title>Release Notes for Worblehat</title>
	</properties>
	<body>

    <!-- see https://maven.apache.org/plugins/maven-changes-plugin/changes.html -->

    <release
      version="1.0"
      date="2011-04-06"
      description="Release with much improved functionality">
      <action dev="betaworblers" type="add">
        Return all borrowed books by one member.
      </action>
      <action dev="betaworblers" type="update">
        Improved borrowing of books.
      </action>
    </release>

    <release
      version="0.9"
      date="2011-04-06"
      description="Second Release with much improved functionality">
      <action dev="betaworblers" type="add">
        Borrowing books.
      </action>
      <action dev="betaworblers" type="fix">
        Spelling errors in the UI.
      </action>
    </release>

    <release
      version="0.8"
      date="2011-04-05"
      description="First release">
			<action
        dev="betaworblers"
        type="add">
				Adding new books.
			</action>
		</release>
	</body>
</document>
